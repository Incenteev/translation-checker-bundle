{
    "name": "incenteev/translation-checker-bundle",
    "type": "symfony-bundle",
    "description": "CLI tools to check translations in a Symfony project",
    "keywords": ["translation check", "translation"],
    "homepage": "https://github.com/Incenteev/IncenteevTranslationCheckerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/console": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/config": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/dependency-injection": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/finder": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/translation": "^3.4.36 || ^4.3 || ^5.0",
        "symfony/yaml": "^3.4.36 || ^4.3 || ^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": { "Incenteev\\TranslationCheckerBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Incenteev\\TranslationCheckerBundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
